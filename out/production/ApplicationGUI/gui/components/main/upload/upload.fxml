<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="256.0" spacing="15.0" styleClass="container" stylesheets="@../app/Application.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.main.upload.UploadController">
   <children>
      <Button id="upload-button" fx:id="LoadFileButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#uploadWorldFromXMLButtonAction" styleClass="config-button" text="Load File">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" top="20.0" />
         </padding></Button>
      <Label fx:id="fileLabel" text="File:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <styleClass>
            <String fx:value="text-holder" />
            <String fx:value="text-holder-bigfont" />
         </styleClass>
      </Label>
      <Label fx:id="fileChosenLabel" styleClass="text-holder" text="Label" />
   </children>
   <padding>
      <Insets left="10.0" />
   </padding>
</HBox>
